[setup]
sqlexception: CREATE TABLE PROCRESTABLE (RES1 BIGINT)
sqlexception: CREATE TABLE MESS (M VARCHAR(100))
sqlexception: CREATE TABLE TEST (SSS CHAR(10))
sqlexception: CREATE TABLE TEST1 (SSS VARCHAR(10), NUM INTEGER)
sqlexception: CREATE TABLE TEST2 (DT DATE)
sql: CREATE OR REPLACE PROCEDURE COUNTMESS(OUT C INTEGER)
P1: BEGIN ATOMIC
  SELECT COUNT(*) INTO C FROM MESS;
 END P1
sql: CREATE OR REPLACE PROCEDURE MYSIGNAL (IN MESS VARCHAR(100))
P1: BEGIN
    SIGNAL SQLSTATE '75000' 
             SET MESSAGE_TEXT = MESS;
END P1
sql: CREATE OR REPLACE PROCEDURE LOGMESS(IN MSS VARCHAR(100))
P1: BEGIN ATOMIC
   INSERT INTO MESS VALUES(MSS);
 END P1
sql: CREATE OR REPLACE PROCEDURE THROWSIGNAL (IN MESS VARCHAR(100))
P1: BEGIN
    IF MESS IS NULL THEN RETURN; END IF;
    SIGNAL SQLSTATE '75000' 
             SET MESSAGE_TEXT = MESS;
END P1
sql: CREATE OR REPLACE FUNCTION ADDNUMBER(IN N1 INTEGER, IN N2 INTEGER) 
  RETURNS INTEGER
P1: BEGIN
  RETURN N1 + N2;
END
